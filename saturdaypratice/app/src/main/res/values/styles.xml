<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextHedingWelcomeBack">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/sp_25</item>
        <item name="android:layout_marginTop">@dimen/sp_15</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="TextHeySofia">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#C4C4C4</item>
        <item name="android:textSize">@dimen/sp_15</item>
    </style>

    <style name="TextMeeting">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/sp_18</item>
        <item name="android:padding">@dimen/dp_10</item>
        <item name="android:layout_marginStart">@dimen/dp_20</item>
    </style>

    <style name="dottedLine">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/dp_10</item>
    </style>

    <style name="TextGraphNumber">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_below">@id/lineOne</item>
        <item name="android:textSize">@dimen/sp_15</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="ButtonGraph">
        <item name="android:src">@drawable/verticalrectangle</item>
        <item name="android:layout_marginStart">@dimen/dp_30</item>
        <item name="android:layout_above">@id/view10</item>
        <item name="android:layout_width">@dimen/dp_10</item>
    </style>

</resources>